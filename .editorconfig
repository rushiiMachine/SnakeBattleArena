# Android Studio: Settings > Editor > Code Style > Enable EditorConfig support

root = true

[*.properties]
ij_properties_align_group_field_declarations = false
ij_properties_keep_blank_lines = true
ij_properties_key_value_delimiter = equals
ij_properties_spaces_around_key_value_delimiter = false

# Allow star imports to remain in the client example
[client-example/**/*.java]
ij_kotlin_name_count_to_use_star_import = 0
ij_kotlin_name_count_to_use_star_import_for_members = 0
